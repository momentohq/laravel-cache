{
  "name": "pgautier404/laravel-cache",
  "description": "Taggable Momento cache driver for Laravel.",
  "type": "library",
  "license": "Apache-2.0",
  "keywords": ["cache", "caching", "serverless"],
  "homepage": "https://github.com/pgautier404/laravel-cache",
  "require": {
    "momentohq/client-sdk-php": "1.1.0"
  },
  "require-dev": {
    "orchestra/testbench": "^7.11"
  },
  "autoload": {
    "psr-4": {
      "Momento\\Cache\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Momento\\Cache\\MomentoServiceProvider"
      ]
    }
  },
  "config": {
    "platform": {
      "php": "8.0.2"
    }
  }
}
