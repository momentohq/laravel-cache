{
  "name": "momentohq/laravel-cache",
  "description": "Taggable Momento cache driver for Laravel.",
  "type": "library",
  "license": "Apache-2.0",
  "keywords": ["cache", "caching", "serverless"],
  "homepage": "https://github.com/momentohq/laravel-cache",
  "authors": [
    {
      "name": "Momento",
      "email": "eng-deveco@momentohq.com",
      "homepage": "https://www.gomomento.com/",
      "role": "Developer"
    }
  ],
  "support": {
    "email": "support@momentohq.com",
    "source": "https://github.com/momentohq/laravel-cache"
  },
  "require": {
    "momentohq/client-sdk-php": "1.3.0"
  },
  "require-dev": {
    "orchestra/testbench": "^7.11"
  },
  "autoload": {
    "psr-4": {
      "Momento\\Cache\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Momento\\Cache\\MomentoServiceProvider"
      ]
    }
  },
  "config": {
    "platform": {
      "php": "8.0.2"
    }
  }
}
